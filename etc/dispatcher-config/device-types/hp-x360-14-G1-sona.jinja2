{% extends 'base-depthcharge.jinja2' %}

{% set start_message = start_message | default('Starting depthcharge on nami...') %}
{% set bootloader_prompt = bootloader_prompt | default('sona:') %}
{% set console_device = console_device | default('ttyS0') %}
{% set boot_retry = 3 %}

{% block timeouts %}
timeouts:
  actions:
    depthcharge-start:
      minutes: 1
{% endblock timeouts %}

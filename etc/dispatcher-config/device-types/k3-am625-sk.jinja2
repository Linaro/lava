{% extends 'base-uboot.jinja2' %}

{% set uboot_mkimage_arch = 'arm64' %}

{% set console_device = console_device|default('ttyS2') %}
{% set baud_rate = baud_rate|default(115200) %}
{% set bootloader_prompt = bootloader_prompt|default('=>') %}

{% set bootz_kernel_addr = '0x82000000' %}
{% set bootz_ramdisk_addr = '0x88080000' %}
{% set bootz_dtb_addr = '0x88000000' %}

{% set bootm_kernel_addr = '0x82000000' %}
{% set bootm_ramdisk_addr = '0x88080000' %}
{% set bootm_dtb_addr = '0x88000000' %}

{% set base_ip_args = 'ip=dhcp' %}
{% set adb_serial_number = adb_serial_number|default("'0000000000'") %}
{% set fastboot_deploy_uboot_commands = fastboot_deploy_uboot_commands|default(["fastboot 0"]) %}
{% set fastboot_sequence = fastboot_sequence|default(['no-flash-boot']) %}

